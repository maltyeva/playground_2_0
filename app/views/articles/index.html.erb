<p id="notice"><%= notice %></p>
<h1>Articles</h1>
<% @articles.each do |article| %>
  <div class="well">
    <%= article.content %>
  </div>
  <div>
    <%= link_to 'Edit', edit_article_path(article) %>
    <%= link_to 'Destroy', article, method: :delete, data: { confirm: 'Are you sure?' } %>
  </div>
<% end %>
<br>
<%= render 'form', article: Article.new %>